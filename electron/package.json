{
  "name": "tp2intervals",
  "productName": "tp2intervals",
  "version": "0.3.0",
  "description": "Third Party synchronization with Intervals.icu",
  "keywords": [
    "trainingpeaks",
    "trainerroad",
    "intervals.icu",
    "sport",
    "cycling",
    "running"
  ],
  "author": {
    "name": "freekode",
    "email": "iam@freekode.org"
  },
  "repository": "https://github.com/freekode/tp2intervals",
  "license": "GNU GPLv3",
  "main": "./out/main/index.js",
  "scripts": {
    "start": "concurrently \"npm run ui:start\" \"wait-on http://localhost:4200 && electron-vite start --sourcemap\"",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run prepare-external-resoruces && electron-vite build && electron-builder --dir",
    "old-package": "npm run prepare-external-resoruces && electron-forge package",
    "old-make": "npm run prepare-external-resoruces && electron-forge make",
    "old-publish": "npm run prepare-external-resoruces && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare-external-resoruces": "npm run jdk:download && npm run ui:build",
    "jdk:download": "run-script-os",
    "jdk:download:darwin:linux": "../devops/jdktool/jdkutil.py clean:jdks download unzip",
    "jdk:download:win32": "python ../devops/jdktool/jdkutil.py clean:jdks download unzip",
    "ui:start": "npm run start --prefix ../ui",
    "ui:build": "npm run build --prefix ../ui -- --base-href=./"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-log": "^5.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.7",
    "typed-emitter": "^2.1.0",
    "update-electron-app": "^3.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@reforged/maker-appimage": "^4.0.2",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^8.2.2",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "electron-forge-maker-appimage": "github:Marcus10110/electron-forge-maker-appimage",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "vite": "^5.1.1",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  }
}
